    <header id="header">
      <div class="container">
         <div class="row">
           <div class="col-md-10">
            <h1><span class="glyphicon glyphicon-cog" aria-hidden="true"></span> Dashboard <small>Grab Your notes</small></h1>
           </div>
           <div class="col-md-2">

           </div>
         </div>
      </div>
    </header>

    <section id="breadcrumb">
      <div class="container">
        <ol class="breadcrumb">
          <li class="active" style="color:white">Dashboard</li>
        </ol>
      </div>
    </section>

    <section id="main">
    <div class="container">

        <div class="col-md-12">
        <!-- website overview-->

          <div class="panel panel-default">
              <div class="panel-heading main-color-bg">
                <h3 class="panel-title">Search Notes</h3>
              </div>
              <div class="panel-body">
                <div class="col-md-3">
                  <div class="well dash-box">
                    <h2><span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span></h2>
                    <a href="#" style="font-size:22px">B.tech sem 1</a>
                  </div>
                </div>
                <div class="col-md-3">
                  <div class="well dash-box">
                    <h2><span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span> </h2>
                    <a href="#" style="font-size:22px">B.tech sem 2</a>
                  </div>
                </div>
                <div class="col-md-3">
                  <div class="well dash-box">
                    <h2><span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span> </h2>
                    <a href="#" style="font-size:22px">B.tech sem 3</a>
                  </div>
                </div>
                <div class="col-md-3">
                  <div class="well dash-box">
                    <h2><span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span> </h2>
                    <a href="#" style="font-size:22px">B.tech sem 4</a>
                  </div>
                </div>
              </div>
            </div>

            <!--latest users-->
            <div class="panel panel-default">
                <div class="panel-heading">Latest Notes</h3>

                </div>
                <div class="panel-body">
                  <ul id="upload">
                    <div class="dropdown">
                    <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                      Search Notes
                      <span class="caret"></span>
                    </button>
                    <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
                      <li><a href="/notesviewbuzzer4%">All Notes</a></li>
                    </ul>
                  </div>
                  <br>
                <h1>Search Now!</h1>
                {{#if success_msg}}
                  <div class="alert alert-success">{{success_msg}}</div>
                {{/if}}

                {{#if error_msg}}
                  <div class="alert alert-danger">{{error_msg}}</div>
                {{/if}}

                {{#if error}}
                  <div class="alert alert-danger">{{error}}</div>
                {{/if}}
                <table class="table  table-striped table-hover" id="table">
                <br>
                <tr>

                  {{#each data}}
                 <td>{{this.uploadfilename}}</td>
                  <td>{{this.Semester}}</td>
<td><a id="submit" href="/login" class="btn btn-primary">download</a>




                </tr>
                </tr>
                  {{/each}}
                </table>
                </ul>
                </div>
              </div>

        </div>
      </div>
    </div>

    </section>


    <footer id="footer">
